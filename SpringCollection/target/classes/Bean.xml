<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id ="sc" class="org.demo.SpringCollections">

<property name="fruits">

<list>
<value>Orange</value>
<value>Apple</value>
<value>Orange</value>
<value>Banana</value>
</list>
</property>


<property name="animals">

<set>
<value>Dog</value>
<value>Cat</value>
<value>Monkey</value>
<value>Dog</value>
</set>

</property>


<property name="empDetails">
<map>

<entry  key="123" value="Vamshi"></entry>
<entry  key="234" value="Srinad">  </entry>
<entry  key="345" value="Reddy">  </entry>
<entry  key="456" value="Pallavi">  </entry>


</map>

</property>

<property name="integers">

<props>
<prop key="1">One</prop>
<prop key="2">Two</prop>
<prop key="3">Three</prop>
<prop key="4">Four</prop>



</props>
</property>
</bean>


</beans>
