<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans   
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


<bean id ="message" class="org.demo.Message">

        
      <property name="em" ref="elementMess"></property>
       <property name="id" value="12345"></property>
       <property name="name" value="Vamshi"></property>

</bean>
<bean id="elementMess" class="org.demo.ElementMessage">
<property name="message" value="I am a Element Message"></property>
</bean>
</beans>